<Window x:Class="KinoMV.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:KinoMV"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
            <RowDefinition />
        </Grid.RowDefinitions>
        <Button Content="Professions" Margin="10" Height="20" Width="100" Click="Prof_Click" />
        <Button Grid.Row="1" Content="Employees" Margin="10" Height="20" Width="100" Click="Employee_Click" />
        <Button Grid.Row="2" Content="Project Employees" Margin="10" Height="20" Width="100" Click="ProjectEmployee_Click" />
    </Grid>
</Window>